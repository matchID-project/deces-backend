{
  "name": "deces-backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint -c .eslintrc.js  --ext .ts src/ --fix",
    "tsoa": "mkdir -p src/routes && mkdir -p src/api && tsc src/tsoa.ts --outDir dist && node dist/tsoa.js",
    "prebuild": "npm run lint && npm run tsoa",
    "build": "tsc --outDir dist",
    "start": "node .",
    "predev": "npm run lint && npm run tsoa",
    "dev": "ts-node-dev --respawn -- src/index.ts",
    "test": "TS_NODE_FILES=true mocha -r ts-node/register --reporter ${REPORTER:-dot} src/*.spec.ts src/**/*.spec.ts --exit",
    "test-grep": "TS_NODE_FILES=true mocha -r ts-node/register --reporter ${REPORTER:-dot} -g ${GREP:-agg} src/*.spec.ts src/**/*.spec.ts --exit"
  },
  "author": "Cristian Brokate",
  "license": "ISC",
  "dependencies": {
    "@jollie/soundex-fr": "^1.0.8",
    "axios": "^1.2.1",
