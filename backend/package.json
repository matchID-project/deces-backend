    "eslint-plugin-jsdoc": "^39.6.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.31.11",
    "mocha": "^10.2.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "=4.8.2"
  }
