{
  "name": "deces-backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint -c .eslintrc.js  --ext .ts src/ --fix",
    "tsoa": "mkdir -p src/routes && mkdir -p src/api && tsc src/tsoa.ts --outDir dist && node dist/tsoa.js",
    "prebuild": "npm run lint && npm run tsoa",
    "build": "tsc --outDir dist",
    "start": "node .",
    "predev": "npm run lint && npm run tsoa",
    "dev": "ts-node-dev --respawn -- src/index.ts",
    "test": "TS_NODE_FILES=true mocha -r ts-node/register --reporter ${REPORTER:-dot} src/*.spec.ts src/**/*.spec.ts --exit",
    "test-grep": "TS_NODE_FILES=true mocha -r ts-node/register --reporter ${REPORTER:-dot} -g ${GREP:-agg} src/*.spec.ts src/**/*.spec.ts --exit"
  },
  "author": "Cristian Brokate",
  "license": "ISC",
  "dependencies": {
    "@jollie/soundex-fr": "^1.0.8",
    "JSONStream": "^1.3.5",
    "axios": "^0.21.1",
    "bee-queue": "^1.3.1",
    "damlev": "^1.0.0",
    "express": "^4.17.1",
    "fast-csv": "^4.3.6",
    "fuzzball": "^1.3.0",
    "fastest-levenshtein": "^1.0.12",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "node-forge": "^0.10.0",
    "streamifier": "^0.1.1",
    "swagger-jsdoc": "^6.0.0",
    "swagger-ui-express": "^4.1.6",
    "swagger-ui-svelte": "latest",
    "tsoa": "^3.4.0",
    "util": "^0.12.3",
    "winston": "^3.3.3",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/chai-http": "^4.2.0",
    "@types/express": "^4.17.9",
    "@types/js-levenshtein": "^1.1.0",
    "@types/mocha": "^8.2.0",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.17",
    "@types/node-forge": "^0.9.5",
    "@types/redis": "^2.8.28",
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-jsdoc": "^30.7.9",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "mocha": "^8.2.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  }
}
