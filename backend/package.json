    "emailjs": "^3.8.1",
    "express": "^4.17.2",
    "fast-csv": "^4.3.6",
    "fastest-levenshtein": "^1.0.16",
    "fuzzball": "^2.1.2",
    "iconv-lite": "^0.6.3",
    "JSONStream": "^1.3.5",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-forge": "^1.3.1",
    "streamifier": "^0.1.1",
    "swagger-jsdoc": "^6.2.7",
    "swagger-ui-express": "^4.6.0",
    "swagger-ui-svelte": "^0.0.13",
    "tsoa": "^4.1.3",
    "util": "^0.12.5",
    "winston": "^3.8.2",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/express": "^4.17.15",
    "@types/ioredis": "^4.28.5",
    "@types/js-levenshtein": "^1.1.1",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/mocha": "^10.0.1",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.5",
    "@types/node-forge": "^1.0.2",
