{
  "name": "deces-backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint -c .eslintrc.js  --ext .ts src/ --fix",
    "tsoa": "mkdir -p src/routes && mkdir -p src/api && tsc src/tsoa.ts --outDir dist && node dist/tsoa.js",
    "prebuild": "npm run lint && npm run tsoa",
    "build": "tsc --outDir dist",
    "start": "node .",
    "predev": "npm run lint && npm run tsoa",
    "dev": "ts-node-dev --respawn -- src/index.ts",
    "test": "TS_NODE_FILES=true BACKEND_TMPFILE_PERSISTENCE=3000 mocha -r ts-node/register --reporter ${REPORTER:-dot} src/*.spec.ts src/**/*.spec.ts --exit",
    "test-grep": "TS_NODE_FILES=true mocha -r ts-node/register --reporter ${REPORTER:-dot} -g ${GREP:-agg} src/*.spec.ts src/**/*.spec.ts --exit"
  },
  "author": "Cristian Brokate",
  "license": "ISC",
  "dependencies": {
    "@jollie/soundex-fr": "^1.0.8",
    "axios": "^1.6.2",
    "bullmq": "^3.15.8",
    "damlev": "^1.0.0",
    "express": "^4.18.2",
    "fast-csv": "^4.3.6",
    "fastest-levenshtein": "^1.0.16",
    "fuzzball": "^2.1.2",
    "iconv-lite": "^0.6.3",
    "JSONStream": "^1.3.5",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-forge": "^1.3.1",
    "nodemailer": "^6.9.7",
    "streamifier": "^0.1.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "swagger-ui-svelte": "^0.0.13",
    "tsoa": "^6.0.0",
    "util": "^0.12.5",
    "winston": "^3.11.0",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@types/chai": "^4.3.10",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^4.28.10",
    "@types/js-levenshtein": "^1.1.3",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mocha": "^10.0.4",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.10",
    "@types/node": "^18.18.9",
    "@types/node-forge": "^1.3.9",
    "@types/nodemailer": "^6.4.14",
    "@types/swagger-jsdoc": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "chai": "^4.3.10",
    "chai-http": "^4.4.0",
    "eslint": "^8.53.0",
    "eslint-plugin-jsdoc": "^39.9.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.33.2",
    "mocha": "^10.2.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  }
}
